    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "clean": "npx rimraf -- hooks node_modules package-lock.json && npm i",
    "build.ionic.key": "nx build ionic-key",
    "prepare.ionic.key": "npm run clean && npm run clean.ionic.key && npm run build.ionic.key",
    "prepare.ionic.key.ios": "npm run prepare.ionic.key && cd apps/ionic-key && npm run cap.add.ios",
    "prepare.ionic.key.android": "npm run prepare.ionic.key && cd apps/ionic-key && npm run cap.add.android",
    "open.ionic.key.ios": "cd apps/ionic-key && npm run cap.ios",
    "open.ionic.key.android": "cd apps/ionic-key && npm run cap.android",
    "sync.ionic.key": "cd apps/ionic-key && npm run cap.copy",
    "clean.ionic.key": "cd apps/ionic-key && npx rimraf -- hooks node_modules platforms www plugins ios android package-lock.json && npm i && rimraf -- package-lock.json"
  },
